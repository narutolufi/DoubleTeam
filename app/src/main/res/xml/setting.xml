<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceCategory android:title="@string/app_name" >
        <ListPreference
            android:entries="@array/server_value"
            android:entryValues="@array/server_value"
            android:key="@string/app_name"
            android:title="@string/app_name" />

        <EditTextPreference
            android:enabled="false"
            android:key="@string/app_name"
            android:title="@string/app_name" />

        <ListPreference
            android:entries="@array/overtime_value"
            android:entryValues="@array/overtime_value"
            android:key="@string/app_name"
            android:title="@string/app_name" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/app_name" >
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="@string/app_name"
            android:title="@string/app_name" />
        <!-- 设置自动同步 -->
        <ListPreference
            android:entries="@array/time_depart"
            android:entryValues="@array/time_depart"
            android:key="@string/app_name"
            android:title="@string/app_name" >
        </ListPreference>
        <ListPreference
            android:entries="@array/themes_value"
            android:entryValues="@array/themes_value"
            android:key="@string/app_name"
            android:title="@string/app_name" >
        </ListPreference>

        <Preference
            android:key="@string/app_name"
            android:title="@string/app_name" />
        <Preference
            android:key="@string/app_name"
            android:title="@string/app_name" />
    </PreferenceCategory>

</PreferenceScreen>